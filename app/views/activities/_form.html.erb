
<%= simple_form_for([@cprocess, @activity], :html => {:multipart => true}) do |f| %>
<%= f.error_notification %>
<%= f.input :order, label: 'Order' %>
<%= f.association :role %>
<%= f.input :title , label: 'Activity Title', as: :text%>
<%= f.input :description, label: 'Activity Description', as: :text  %>
<%= f.submit%>
<% end %>